{
    "welcomeFile": "/airports/webapp/index.html",
    "authenticationMethod": "route",
    "sessionTimeout": 60,
    "routes": [
        {
            "source": "^/odata/v4/(.*)$",
            "target": "/odata/v4/$1",
            "destination": "srv-api",
            "authenticationType": "xsuaa",
            "csrfProtection": true
        },
        {
            "source": "^(.*)$",
            "target": "/$1",
            "localDir": ".",
            "authenticationType": "xsuaa"
        }
    ]
}
