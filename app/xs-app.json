{
    "welcomeFile": "/fuelsphere.airports/index.html",
    "authenticationMethod": "route",
    "sessionTimeout": 60,
    "routes": [
        {
            "source": "^/odata/v4/(.*)$",
            "target": "/odata/v4/$1",
            "destination": "srv-api",
            "authenticationType": "xsuaa",
            "csrfProtection": true
        },
        {
            "source": "^/fuelsphere\\.airports/(.*)$",
            "target": "/fuelsphere.airports/$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^(.*)$",
            "target": "/$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        }
    ]
}
