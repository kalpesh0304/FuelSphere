{
  "_version": "1.59.0",
  "sap.app": {
    "id": "fuelsphere.airports",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/master/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "airports-display": {
          "semanticObject": "Airports",
          "action": "display",
          "title": "{{appTitle}}",
          "icon": "sap-icon://flight",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://flight",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fuelsphere.airports.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "AirportsList",
          "target": "AirportsList"
        },
        {
          "pattern": "Airports({key}):?query:",
          "name": "AirportsObjectPage",
          "target": "AirportsObjectPage"
        }
      ],
      "targets": {
        "AirportsList": {
          "type": "Component",
          "id": "AirportsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Airports",
              "variantManagement": "Page",
              "navigation": {
                "Airports": {
                  "detail": {
                    "route": "AirportsObjectPage"
                  }
                }
              }
            }
          }
        },
        "AirportsObjectPage": {
          "type": "Component",
          "id": "AirportsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "contextPath": "/Airports",
              "editableHeaderContent": false
            }
          }
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "fuelsphere"
  }
}
